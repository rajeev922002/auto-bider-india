											<button class="tab" data-tab="hourly">Hourly</button>
										</div>
										<div class="tab-content active" id="fixed">
											<!-- Micro Project -->
											<div class="budget-range">
												<h3>Micro project</h3>
												<div class="budget-inputs">
													<label><input type="radio" name="micro-project-budget" value="lowest" <%= biddingPrice && biddingPrice.micro_project && biddingPrice.micro_project.budget === 'lowest' ? 'checked' : '' %>> Lowest</label>
													<label><input type="radio" name="micro-project-budget" value="average" <%= biddingPrice && biddingPrice.micro_project && biddingPrice.micro_project.budget === 'average' ? 'checked' : '' %>> Average</label>
													<label><input type="radio" name="micro-project-budget" value="highest" <%= biddingPrice && biddingPrice.micro_project && biddingPrice.micro_project.budget === 'highest' ? 'checked' : '' %>> Highest</label>
													<div class="input-group">
														<label>10 - 30 USD / AUD / CAD</label>
														<input type="number" name="micro-project-usd-aud-cad-10-30" placeholder="Bid Price" value="<%= biddingPrice && biddingPrice.micro_project ? biddingPrice.micro_project.bid_usd_aud_cad || '' : '' %>">
													</div>
													<div class="input-group">
														<label>10 - 20 GBP</label>
														<input type="number" name="micro-project-gbp-10-20" placeholder="Bid Price" value="<%= biddingPrice && biddingPrice.micro_project ? biddingPrice.micro_project.bid_gbp || '' : '' %>">
													</div>
													<div class="input-group">
														<label>600 - 1500 INR</label>
														<input type="number" name="micro-project-inr-600-1500" placeholder="Bid Price" value="<%= biddingPrice && biddingPrice.micro_project ? biddingPrice.micro_project.bid_inr || '' : '' %>">
													</div>
													<div class="input-group">
														<label>8 - 30 EUR</label>
														<input type="number" name="micro-project-eur-8-30" placeholder="Bid Price" value="<%= biddingPrice && biddingPrice.micro_project ? biddingPrice.micro_project.bid_eur || '' : '' %>">
													</div>
													<div class="input-group">
														<label>14 - 30 NZD</label>
														<input type="number" name="micro-project-nzd-14-30" placeholder="Bid Price" value="<%= biddingPrice && biddingPrice.micro_project ? biddingPrice.micro_project.bid_nzd || '' : '' %>">
