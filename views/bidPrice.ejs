		body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    background-color: #f4f4f4;
}
		.container {
    display: flex;
}

.sidebar {
    width: 200px;
    background-color: #fff;
    padding: 15px;
    box-shadow: 0 0 10px rgba(0,0,0,0.1);
}

.settings {
    list-style-type: none;
    padding: 0;
}

.settings li {
    padding: 10px 0;
    border-bottom: 1px solid #ddd;
    cursor: pointer;
}

.settings li.active {
    font-weight: bold;
    color: #007bff;
}

.main-content {
    flex: 1;
    padding: 20px;
}

.manage-bid-price h1 {
    margin-top: 0;
}

.range-inputs label, .budget-inputs .input-group {
    display: block;
    margin-bottom: 10px;
}

.range-inputs input, .budget-inputs input {
    width: 100%;
    padding: 8px;
    margin-top: 5px;
    box-sizing: border-box;
}

.tabs {
    display: flex;
    border-bottom: 1px solid #ddd;
    margin-bottom: 20px;
}

.tab {
    flex: 1;
    padding: 10px;
    text-align: center;
    cursor: pointer;
    border: 1px solid #ddd;
    border-bottom: none;
    background-color: #f9f9f9;
}

.tab.active {
    background-color: #fff;
    font-weight: bold;
}

.tab-content {
    display: none;
}

.tab-content.active {
    display: block;
}

.budget-range {
    margin-bottom: 20px;
}

.budget-range h3 {
    margin: 0 0 10px;
}

.input-group label {
    margin: 0;
}

.submit-btn {
    display: block;
    width: 100%;
    padding: 10px;
    background-color: #007bff;
    color: #fff;
    border: none;
    cursor: pointer;
    text-align: center;
}

.submit-btn:hover {
    background-color: #0056b3;
}
