	<script type="text/javascript">
		var totalRows = <%= periods.length %> + 1;
		$(document).ready(function () {
			$(document).on('click', '.btn-add-row', function () {
				$('.table-range tbody').append(`<tr>
					<td class="align-middle text-center" style="width: 50px">`+ totalRows + `</td>
					<td class="align-middle" style="min-width: 150px"><input type="number" class="form-control form-control-sm" min="0" step="1" name="lower[`+ totalRows + `]" placeholder="Lower Bid Price" required></td>
					<td class="align-middle" style="min-width: 150px"><input type="number" class="form-control form-control-sm" min="0" step="1" name="higher[`+ totalRows + `]" placeholder="Higher Bid Price" required></td>
					<td class="align-middle" style="min-width: 150px"><input type="number" class="form-control form-control-sm" min="0" step="1" name="period[`+ totalRows + `]" placeholder="Project Period" required></td>
					<td class="align-middle text-center" style="width: 50px"><button type="button" tabindex="-1" class="btn btn-danger btn-sm btn-block btn-remove-row"><i class="fa fa-times"></i></button></td>
					</tr>`);
				totalRows++;
			});

			$(document).on('click', '.btn-remove-row', function () {
				$(this).parents('tr').remove();
			});
		});
	</script>
