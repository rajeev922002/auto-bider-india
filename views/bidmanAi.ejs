	<script>
		document.addEventListener("DOMContentLoaded", function () {
    var modal = document.getElementById("popupModal");
    var btn = document.getElementById("claimButton");
    var span = document.getElementsByClassName("close")[0];

    // When the user clicks the button, open the modal
    btn.onclick = function () {
        modal.style.display = "block";
    }

    // When the user clicks on <span> (x), close the modal
    span.onclick = function () {
        modal.style.display = "none";
    }

    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function (event) {
        if (event.target == modal) {
            modal.style.display = "none";
        }
    }

    // Submit form
    document.getElementById("infoForm").addEventListener("submit", function (event) {
        event.preventDefault();
        var phone = document.getElementById("phone").value;
        var email = document.getElementById("email").value;

        // Axios call
        axios.post('/getBonus', {
            phone: phone,
            email: email
        })
        .then(function (response) {
            // Request was successful
            console.log(response.data); // Log the response from the server
            modal.style.display = "none"; // Close the modal after submission
            showToast("Bonus Availed", true); // Show success toast
        })
        .catch(function (error) {
            // Request failed
            console.error(error);
            modal.style.display = "none";
            showToast("Sorry, you already availed your Bonus", false); // Show error toast
        });
    });

    // Function to show toast (for demonstration, you can customize this)
    function showToast(message, isSuccess) {
        var toast = document.createElement("div");
        toast.className = "toast";
        toast.style.backgroundColor = isSuccess ? "green" : "red";
        toast.innerText = message;
        document.body.appendChild(toast);
        setTimeout(function () {
            toast.remove();
        }, 3000);
    }
});
	</script>
