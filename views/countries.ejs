    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Safely get the excluded countries from the server
            var excludedCountries = <%- JSON.stringify(excluded_countries) %>; 

            console.log(excludedCountries);
            
            // Fetch all checkboxes
            var checkboxes = document.querySelectorAll('input[name="countries[]"]');
            
            // Set checkboxes as checked if they are in the excluded countries
            checkboxes.forEach(function (checkbox) {
                if (excludedCountries.includes(checkbox.value)) {
                    checkbox.checked = true;
                }
            });

            // Trigger the toggle to refresh UI - if using bootstrap toggle
            $(checkboxes).bootstrapToggle('update');
        });
    </script>
