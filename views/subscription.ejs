        <script type="text/javascript">
            $(document).ready(function () {
                var datatable = $('#table-list').DataTable({
                    "responsive": true,
                    "autoWidth": false,
                    "order": [[0, "asc"]],
                    "columnDefs": [
                        {
                            "orderable": false,
                            "searchable": false,
                            "targets": [2, 3]
                        },
                    ],
                    "pageLength": 30,
                    "lengthMenu": [[10, 20, 30, 50, -1], [10, 20, 30, 50, "All"]],
                });
                new $.fn.dataTable.FixedHeader(datatable);

                $(document).on('click', '.generate-bid-template-skill', function () {
                    var btn = $(this);
                    $.ajax({
                        type: "post",
                        url: "get_random_bid_template_skill.php",
                        data: 'skill_id=' + btn.data('id') + '&name=' + btn.data('name'),
                        beforeSend: function () {
                            btn.html('<i class="fa fa-spinner fa-spin"></i>');
                            btn.prop('disabled', 'disabled');
                            $('#bidModal .modal-body').html('Loading...');
                            $('#bidModal .modal-title').html(btn.data('name'));
                            $('#bidModal').modal('show')
                        }
                    })
                        .done(function (Response) {
                            console.log(Response);
                            if (Response != 'ERROR') {
                                $('#bidModal .modal-body').html(Response['button'] + Response['categories'] + Response['bid']);
                            }
                            else {
                                toastr["error"]('Error generating random bid');
                            }
                        })
                        .fail(function () {

                        })
                        .always(function () {
                            btn.html('Generate Bid');
                            btn.prop('disabled', '');
                        });
                });
            });
            document.addEventListener('click', function (event) {
                if (event.target.classList.contains('delete-template')) {
                    const templateId = event.target.dataset.templateId;
                    deleteTemplate(templateId);
                }
            });

