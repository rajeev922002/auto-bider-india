            justify-content: center;
            align-items: center;
            margin-right: 4px;
            -webkit-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

        .priority-box-name {
            -webkit-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

        .toggle.btn {
            min-height: 28px;
        }

        .toggle-on,
